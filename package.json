{
  "name": "ruby",
  "version": "1.0.0",
  "description": "Ruby, a private moderation bot!",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --rs --cls src/index.ts",
    "build": "tsc --project tsconfig.json",
    "prod:linux": "chmod 777 src/scripts/start.sh && chmod 777 node_modules/.bin/ts-node-dev && pm2 start src/scripts/start.sh --name=Helper",
    "build:prod": "npm run build && npm run prod"
  },
  "keywords": [
    "discord bot",
    "discord.js",
    "typescript",
    "coding",
    "support",
    "modmail",
    "discord"
  ],
  "author": "Saige",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.1",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "glob": "^7.2.0",
    "mongoose": "^6.2.10",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.23",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
